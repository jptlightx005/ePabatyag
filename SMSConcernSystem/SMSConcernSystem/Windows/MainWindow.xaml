<Window x:Class="MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="MainWindow" Height="350" Width="525">
    <Grid Background="#FFB2D6F9">
        <DataGrid Margin="10,37,10,10"/>
        <Button x:Name="btnLogout" Content="Logout" HorizontalAlignment="Right" VerticalAlignment="Top" Width="75" Margin="0,10,10,0"/>
        <Button x:Name="btnRegister" Content="Register" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,10,0,0"/>
        <Button x:Name="btnStudentList" Content="Student List" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="90,10,0,0"/>
        <Button x:Name="btnAbout" Content="About" HorizontalAlignment="Right" VerticalAlignment="Top" Width="75" Margin="0,10,90,0"/>

    </Grid>
</Window>
