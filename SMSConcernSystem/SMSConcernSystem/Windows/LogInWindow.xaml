<Window x:Class="LogInWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Log_in" Height="333" Width="361.777">
    <Grid Background="#FFB2D6F9">
        <Button x:Name="btnLogin" Content="Log-in" HorizontalAlignment="Right" VerticalAlignment="Top" Width="75" Margin="0,136,25,0" FontSize="14"/>
        <TextBox x:Name="txtUsrn" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Margin="87,72,25,0" FontSize="14"/>
        <PasswordBox x:Name="txtPssw" VerticalAlignment="Top" Margin="87,108,25,0" Height="23" FontSize="14"/>
        <Label Content="Username" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,70,0,0" FontSize="14"/>
        <Label Content="Password" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="14,104,0,0" FontSize="14"/>

    </Grid>
</Window>
